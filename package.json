{
  "name": "electron-react-typescript-webpack-boilerplate",
  "version": "2.0.2",
  "description": "Pre-configured boilerplate for Electron + React + TypeScript",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Devtography/electron-react-typescript-webpack-boilerplate"
  },
  "homepage": "https://inceptmedical.com",
  "main": "./dist/main.bundle.js",
  "scripts": {
    "debug": "rimraf dist && cross-env NODE_ENV=development DEBUG=true webpack --watch --progress --colors",
    "dev": "rimraf dist && cross-env NODE_ENV=development webpack --watch --progress --colors",
    "prod": "rimraf dist && cross-env NODE_ENV=production webpack --progress --colors",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register \"test/**/*.ts\"",
    "build:win": "electron-builder build --win --x64",
    "build:mac": "electron-builder build --mac --x64"
  },
  "build": {
    "appId": "com.devtography.electron_boilerplate",
    "productName": "Electron+React+TypeScript Boilerplate",
    "directories": {
      "app": "./dist/",
      "output": "./out/"
    },
    "mac": {
      "target": "pkg"
    },
    "win": {
      "target": "nsis",
      "asar": false
    },
    "buildVersion": "1.0.0"
  },
  "author": "Wing Chau @Devtography",
  "license": "MIT",
  "dependencies": {
    "connected-react-router": "^6.8.0",
    "react": "^16.12.0",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.1",
    "@types/mocha": "^5.2.7",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router": "^4.4.5",
    "@types/react-router-dom": "^4.3.5",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "acorn": "^6.4.0",
    "ajv": "^6.10.2",
    "babel-loader": "^8.1.0",
    "copy-pkg-json-webpack-plugin": "0.0.38",
    "cross-env": "^5.2.1",
    "css-loader": "^1.0.1",
    "electron": "^3.1.13",
    "electron-builder": "^20.44.4",
    "electron-connect": "^0.6.3",
    "electron-devtools-installer": "^3.1.0",
    "electron-reload-webpack-plugin": "^2.0.4",
    "eslint": "^7.3.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.2",
    "file-loader": "^2.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "lodash": "^4.17.15",
    "mocha": "^5.2.0",
    "prettier": "^2.0.5",
    "redux-devtools-extension": "^2.13.8",
    "rimraf": "^2.7.1",
    "source-map-loader": "^0.2.4",
    "spectron": "^5.0.0",
    "style-loader": "^0.23.1",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-prettier": "^1.1.2",
    "ts-loader": "^5.4.5",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "tslint-microsoft-contrib": "^5.2.1",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint"
    ],
    "{*.json,.{babelrc,eslintrc,prettierrc,stylelintrc}}": [
      "prettier --ignore-path .eslintignore --parser json --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "renovate": {
    "extends": [
      "bliss"
    ]
  }
}
