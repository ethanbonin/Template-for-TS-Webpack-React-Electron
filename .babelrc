{
    "presets": [
        "@babel/typescript",
        "@babel/preset-react",
        "@babel/preset-env",
        ["env", { "modules": false }]
    ],
    "plugins": [
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread",
        "syntax-dynamic-import"
    ],
    "env": {
        "test": {
            "plugins": ["dynamic-import-node"]
        }
    }
}
